{
    "name": "miliband-components",
    "version": "1.0.0",
    "description": "",
    "main": "index.js",
    "scripts": {
        "build": "rollup -c",
        "clear": "if [ -d docs/dist ]; then rm -Rf docs/dist; fi;",
        "postbuild": "yarn clear; cp -R dist docs/dist",
        "predev": "yarn clear; cd docs; ln -s ../dist; cd ../",
        "dev": "rollup -c -w",
        "serve": "docsify serve ./docs"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/JacobWeinbren/miliband-components.git"
    },
    "keywords": [
        "custom-components",
        "storybook"
    ],
    "prettier": {
        "trailingComma": "es5",
        "tabWidth": 4,
        "semi": true,
        "singleQuote": false
    },
    "browserslist": [
        "> 1%",
        "last 2 versions"
    ],
    "author": "Jacob",
    "license": "MIT",
    "dependencies": {
        "@babel/core": "^7.15.5",
        "@emotion-icons/bootstrap": "^3.14.0",
        "@rollup/plugin-babel": "^5.3.0",
        "@rollup/plugin-image": "^2.1.1",
        "@rollup/plugin-node-resolve": "^13.0.5",
        "autoprefixer": "^10.3.7",
        "esbuild": "^0.13.4",
        "lit": "^2.0.2",
        "postcss": "^8.3.9",
        "rollup": "^2.58.0",
        "rollup-plugin-copy": "^3.4.0",
        "rollup-plugin-esbuild": "^4.5.0",
        "rollup-plugin-postcss": "^4.0.1",
        "rollup-plugin-postcss-lit": "^2.0.0",
        "rollup-plugin-terser": "^7.0.2",
        "sass": "^1.42.1"
    }
}
